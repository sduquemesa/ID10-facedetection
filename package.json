{
  "name": "id10",
  "version": "1.0.0",
  "description": "Source code for a face-detection art webapp",
  "homepage": "https://sduquemesa.github.io/ID10-facedetection",
  "main": "index.js",
  "scripts": {
    "dev": "parcel index.html",
    "build": "parcel build index.html --public-url /ID10-facedetection",
    "build-dev": "parcel build index.html --no-minify --public-url ./",
    "predeploy": "rm -rf dist && npm run build",
    "deploy": "gh-pages -d dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sduquemesa/ID10-facedetection"
  },
  "keywords": [
    "ml5",
    "p5js",
    "tonejs",
    "imagefest"
  ],
  "author": "sduquemesa",
  "license": "GPL-3.0",
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "gh-pages": "^3.1.0",
    "parcel-bundler": "^1.12.5"
  },
  "dependencies": {
    "hydra-synth": "^1.3.8",
    "jsonpack": "^1.1.5",
    "ml5": "^0.6.1",
    "p5": "^1.3.1",
    "qrcode": "^1.4.4"
  }
}
